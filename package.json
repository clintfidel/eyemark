    "eslint-plugin-react": "^7.22.0",
    "moxios": "^0.4.0",
    "netlify-cli": "^3.13.0",
    "nightwatch": "^1.6.4",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "i18next-scanner": "^3.0.0",
