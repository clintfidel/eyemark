  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "client:test": "jest --coverage",
    "eject": "react-scripts eject",
    "scan-translations": "i18next-scanner",
    "netlify:deploy": "netlify deploy --dir=./build -p -m \"$(git log -1 --pretty=%B)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/test/**/*.test.js?(x)"
    ],
    "verbose": true,
    "collectCoverage": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "codeclimate-test-reporter": "^0.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
