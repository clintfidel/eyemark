    "jest": "^27.0.4",
    "node-sass": "4.14.1",
    "nyc": "^15.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.5",
    "react-image-crop": "^8.6.12",
    "react-power-tooltip": "^1.0.2",
